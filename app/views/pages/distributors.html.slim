.distributors ng-controller="dustrubutorsControler as stor"
  .row
    .medium-3.columns
      .selector.bage
        / = render "selector"
        .select
          .accordion-button
            p  countries
            .svg-slid = inline_svg "svg/ic_keyboard_arrow_up_black_24px.svg"
          .accordion-inner.show

            .selector
              / input#post-2.read-more-state type="checkbox"
              form name="userForm" ng-submit="submitForm()"
                ul.read-more-wrap ng-repeat="countries in all_countries"
                  li
                    input type="checkbox" ng-model="countries.selected" ng-change='trackOrder(countries)'  value="{{countries.id}}" ng-click="submitForm().submit()"  {{countries.title}}
      .dubl-select
        .accordion-button.opened
          p afdsfdsf
          .svg-slid = inline_svg "svg/ic_keyboard_arrow_down_black_24px.svg"
        .accordion-inner.show

          .selector
            ul.read-more-wrap ng-repeat="distributor in questiondistributors"
              li
                .one-checkbox
                  .checkbox
                    input#checkbox-salis1 type="checkbox"
                    label.switcher-control for="checkbox-salis1"
                    span
                      a href="distributors?id={{distributor.id}}"
                        | {{distributor.name}}
                    nav
                      ul.drop-down.closed
                        li
                          a.nav-button href="#" ng-click="voteUp" = inline_svg "svg/ic_keyboard_arrow_down_black_24px.svg"
                        .down
                          ul ng-repeat="second_menu in distributor.second_menu"
                            li
                              .one-checkbox
                                .checkbox
                                  input#checkbox-salis6 type="checkbox"
                                  label.switcher-control for="checkbox-salis6"
                                  span
                                    a href="distributors?second_menu_id={{second_menu.id}}"
                                      | {{second_menu.title}}
          .remove-all
            .x-component = inline_svg "photo/main/svg/filter_clear.svg"
            .ramove-text
              a href=distributors_path очистить все
            /// smole select
      .wrapper-filter
        .openfilters
          p фильтра
        .filter-container
          = render "selectsmole"
          .dubl-select
            .accordion-button.opened
              p afdsfdsf
              .svg-slid = inline_svg "svg/ic_keyboard_arrow_up_black_24px.svg"
            .accordion-inner.show

              .selector
                ul.read-more-wrap
                  li
                    .one-checkbox
                      .checkbox
                        input#checkbox-salis1 type="checkbox"
                        label.switcher-control for="checkbox-salis1"
                        span продажа
                        nav
                          ul.drop-down.closed
                            li
                              a.nav-button href="#"   = inline_svg "svg/ic_keyboard_arrow_down_black_24px.svg"
                            .down
                              ul
                                li
                                  .one-checkbox
                                    .checkbox
                                      input#checkbox-salis6 type="checkbox"
                                      label.switcher-control for="checkbox-salis6"
                                      span продажа
                                li
                                  .one-checkbox
                                    .checkbox
                                      input#checkbox-salis7 type="checkbox"
                                      label.switcher-control for="checkbox-salis7"
                                      span продажа
                                li
                                  .one-checkbox
                                    .checkbox
                                      input#checkbox-salis8 type="checkbox"
                                      label.switcher-control for="checkbox-salis8"
                                      span продажа
                  li
                    .one-checkbox
                      .checkbox
                        input#checkbox-salis2 type="checkbox"
                        label.switcher-control for="checkbox-salis2"
                        span продажа
                        nav
                          ul.drop-down.closed
                            li
                              a.nav-button href="#"   = inline_svg "svg/ic_keyboard_arrow_down_black_24px.svg"
                            li
                              .one-checkbox
                                .checkbox
                                  input#checkbox-salis9 type="checkbox"
                                  label.switcher-control for="checkbox-salis10"
                                  span продажа
                            li
                              .one-checkbox
                                .checkbox
                                  input#checkbox-salis11 type="checkbox"
                                  label.switcher-control for="checkbox-salis11"
                                  span продажа
                            li.
                              .one-checkbox
                                .checkbox
                                  input#checkbox-salis12 type="checkbox"
                                  label.switcher-control for="checkbox-salis12"
                                  span продажа

                  li
                    .one-checkbox
                      .checkbox
                        input#checkbox-salis3 type="checkbox"
                        label.switcher-control for="checkbox-salis3"
                          span продажа
                  li
                    .one-checkbox
                      .checkbox
                        input#checkbox-salis4 type="checkbox"
                        label.switcher-control for="checkbox-salis4"
                          span продажа
                  li
                    .one-checkbox
                      .checkbox
                        input#checkbox-salis5 type="checkbox"
                        label.switcher-control for="checkbox-salis5"
                          span продажа
              .remove-all
                .x-component = inline_svg "photo/main/svg/filter_clear.svg"
                .ramove-text очистить все
        /// smole select
        .selector
          = render "advertisingSlider"

    .medium-9.columns
      .topgoods
        .topgoods-tex.color-line.gray-block
          .title топ товари
        #sliderBX-contenedor
          #btnSliderNext
            i.fa.fa-angle-right aria-hidden="true"
          #btnSliderPrev
            i.fa.fa-angle-left aria-hidden="true"
          #sliderBX
            .sliderBXcell ng-repeat="tdistributors_top in toptdistributors"
              / .medium-4.columns
              .border-producers.color-line.green-block
                .one-new-producers
                  a.img-bg href="distributor/{{tdistributors_top.id}}" style="background-image: url('{{tdistributors_top.image_url}}')"
                .description
                  .description-txt
                    .maker
                      | {{tdistributors_top.country}}
                    .product-name
                      | {{tdistributors_top.title}}
            .sliderBXcell
            .sliderBXcell
            .sliderBXcell

    .medium-9.columns
      .salse-all
        .sales-leaders ng-repeat="distributorall in alldistributors"
          .medium-4.columns
            .border-producers.color-line.green-block
              .one-new-producers
                / href="distributors?id={{distributor.id}}"
                a.img-bg href="distributor/{{distributorall.id}}" style="background-image: url('{{distributorall.image_url}}')"
              .description
                .description-txt
                  .maker
                    | {{distributorall.country}}
                  .product-name
                    | {{distributorall.title}}
    / .medium-9.columns
    /   .salse-all
    /     -@distributors.each do |distributor|
    /       .medium-4.columns
    /           .border-producers.color-line.green-block
    /             .one-new-producers
    /               a.img-bg href=distributor_path(distributor.id) style="background-image: url(#{distributor.logo_image&.url})"
    /             .description
    /               .description-txt
    /                 .maker
    /                   =distributor.country&.title
    /                 .product-name
    /                   =distributor.title
    .medium-9.columns
      = render "recomendationdistributors"
    / .medium-9.columns
    /   .salse-all
    /     .medium-4.columns  ng-repeat="product in records | limitTo : 6"
    /       .border-producers.color-line.green-block
    /         .one-new-producers
    /           .img-bg style="background-image: url(#{asset_path('photo/main/logos/2677650ac2e6996116659b8a3f91438a.png')})"
    /         .description
    /           .description-txt
    /             .maker
    /               | {{product.maker}}
    /             .product-name
    /               | {{product.product}}
