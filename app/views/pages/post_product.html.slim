      #goods.tabcontent
        form.ajax-form  action="/creating_unit" method="post" enctype="multipart/form-data" name="usubmitForm" ng-submit="submitForm()" novalidate=""
          category
          input.d-none type="text" value='products' name="category"
          / tariff
          .form-group ng-class=("{ 'has-error' : usubmitForm.tariff.$invalid && !usubmitForm.tariff.$pristine }")
            .lable-one
              .label
                = t("post_product.tariff_plan")
              / .span  *
            select#nazn.selectid.form-control name="selected_fare" required="" ng-model="user.tariff" type="text"
              //ng-model="user.tariff"
              option#one1.sel_dok value="free"  free
              option#two1.sel_dok value="basic"  basic
              option#three1.sel_dok value="platinum"  platinum

          name
          .form-group ng-class=("{ 'has-error' : usubmitForm.name.$invalid && !usubmitForm.name.$pristine }")
            .lable-one
              .label
                = t("post_product.company_name")
              / .span  *
            input.text-input.form-control name="name" ng-model="user.name" required="" type="text" minlength="5" placeholder= t("post_product.сompany_placeholder")
            p.help-block ng-show=("usubmitForm.name.$invalid && !usubmitForm.name.$pristine")  You name is required.
          /
          /
          /
          /
          //manufacturers
          .form-group ng-class=("{ 'has-error' : usubmitForm.tariff.$invalid && !usubmitForm.tariff.$pristine }")
            .lable-one
              .label
                = t("post_product.type_of_production")
              / .span  *
            select#nazn.selectid.form-control name="Type_production" required="" ng-model="user.Type_production" type="text" ng-options="item.name for item in questionfactories"
              / option питания
              / option косметика
              / option для дома


          /logotype
          .form-group ng-class=("{ 'has-error' : usubmitForm.tariff.$invalid && !usubmitForm.tariff.$pristine }")
            .lable-one
              .label
                = t("post_product.Logotype")

            input#upload.custom-file-input.none name='logo' ng-file-select="onFileSelect($files)"  ng-model="user.imageSrc" required=""  type="file" accept=".jpg, .jpeg, .png"
            .img
              img.content-centering_element ng-src="{{user.imageSrc}}"

          /aboyut as
          .form-group ng-class=("{ 'has-error' : usubmitForm.comment.$invalid && !usubmitForm.comment.$pristine }")
            .lable-one
              .label
                = t("post_product.Description")
            textarea.form-control.ng-pristine.ng-valid.ng-empty.ng-touched name="comment" ng-model="user.comment" placeholder=("") rows="5" minlength="15"
            p.help-block ng-show="usubmitForm.comment.$error.maxlength"  not less than 15 characters

          // foto
          .form-group ng-class=("{ 'has-error' : usubmitForm.username.$invalid && !usubmitForm.username.$pristine }")
            .lable-one
              .label
                = t("post_product.photo")
            / div.target
            /   div  img-upload=""

            .wrap-custom-file
              .image1
                input#image1.olt accept=".gif, .jpg, .png" name="photo[]" type="file" /
                label for="image1"
                  span Select Image One
            .wrap-custom-file
              .image2
                input#image2.olt accept=".gif, .jpg, .png" name="photo[]" type="file" /
                label for="image2"
                  span Select Image Two
            .wrap-custom-file
              .image3
                input#image3.olt accept=".gif, .jpg, .png" name="photo[]" type="file" /
                label for="image3"
                  span Select Image Three
            .wrap-custom-file
              .image4
                input#image4.olt accept=".gif, .jpg, .png" name="photo[]" type="file" /
                label for="image4"
                  span Select Image Four
            .wrap-custom-file
              .image5
                input#image5.olt accept=".gif, .jpg, .png" name="photo[]" type="file" /
                label for="image5"
                  span Select Image fifth
            .wrap-custom-file
              .image6
                input#image6.olt accept=".gif, .jpg, .png" name="photo[]" type="file" /
                label for="image6"
                  span Select Image fifth
              /video
          .form-group ng-class=("{ 'has-error' : usubmitForm.video.$invalid && !usubmitForm.video.$pristine }")
            .lable-one
              .label
                = t("post_product.link_to_video")
              / .span  *
            input.text-input.form-control name="link_video" ng-model="user.link_video" required="" type="text" minlength="5" placeholder="https://www.youtube.com/watch" /
            p.help-block ng-show=("usubmitForm.video.$invalid && !usubmitForm.video.$pristine")  You name is required.
          / catalog
          .form-group.fails ng-class=("{ 'has-error' : usubmitForm.catalog.$invalid && !usubmitForm.catalog.$pristine }")
            .lable-one
              .label
                = t("post_product.product_Catalog")
            input#upload.custom-file-input name="catalog" ng-model="user.catalog"  type="file"
          // list
          .form-group.fails ng-class=("{ 'has-error' : usubmitForm.list.$invalid && !usubmitForm.list.$pristine }")
            .lable-one
              .label
                = t("post_product.price_list")
            input#upload.custom-file-input name="price_list" ng-model="user.list"  type="file"
          // sert
          .form-group.fails ng-class=("{ 'has-error' : usubmitForm.sert.$invalid && !usubmitForm.sert.$pristine }")
            .lable-one
              .label
                = t("post_product.certificate")
            input#upload.custom-file-input name="certificate" ng-model="user.sert"  type="file"

          .taitle-contacts.color-line.yellow-block
            = t("post_product.additional_Information")
          //country
          .form-group ng-class=("{ 'has-error' : usubmitForm.country.$invalid && !usubmitForm.country.$pristine }")
            .lable-one
              .label
                = t("post_product.country")
              / .span  *
            input.text-input.form-control name="country" ng-model="user.country" required="" type="text" minlength="3" placeholder= t("post_product.country")
            p.help-block ng-show=("usubmitForm.country.$invalid && !usubmitForm.country.$pristine")  You country is required.
          // city
          .form-group ng-class=("{ 'has-error' : usubmitForm.city.$invalid && !usubmitForm.city.$pristine }")
            .lable-one
              .label
                = t("post_product.city")
              / .span  *
            input.text-input.form-control name="city" ng-model="user.city" required="" type="text" minlength="3" placeholder= t("post_product.city")
            p.help-block ng-show=("usubmitForm.city.$invalid && !usubmitForm.city.$pristine")  You country is required.
          // street
          .form-group ng-class=("{ 'has-error' : usubmitForm.street.$invalid && !usubmitForm.street.$pristine }")
            .lable-one
              .label
                = t("post_product.street")
              / .span  *
            input.text-input.form-control name="street" ng-model="user.street" required="" type="text" minlength="3" placeholder= t("post_product.street")
            p.help-block ng-show=("usubmitForm.street.$invalid && !usubmitForm.street.$pristine")  You country is street.
          // phone
          .form-group ng-class=("{ 'has-error' : usubmitForm.phone.$invalid && !usubmitForm.phone.$pristine }")
            .lable-one
              .label
                = t("post_product.phone")
            input.text-input.form-control required="" name="phone" ng-model="user.phone" type="text"  placeholder="+91-636-78658" ng-pattern="phoneNumbr"

            p.help-block.phone ng-show="usubmitForm.phone.$error.pattern"  Please match pattern [+91 036 78658 or 91 036 78658]
          // site
          .form-group ng-class=("{ 'has-error' : usubmitForm.site.$invalid && !usubmitForm.site.$pristine }")
            .lable-one
              .label
                = t("post_product.site")
              / .span  *
            input.text-input.form-control name="site" ng-model="user.site" required="" type="text" minlength="5" placeholder="www.site.com"
            p.help-block ng-show=("usubmitForm.site.$invalid && !usubmitForm.site.$pristine")  You site is street.
          .taitle-contacts.color-line.yellow-block
            = t("post_product.the_contact_person")
          // fio
          .form-group ng-class=("{ 'has-error' : usubmitForm.fio.$invalid && !usubmitForm.fio.$pristine }")
            .lable-one
              .label
                = t("post_product.full_name")
              / .span  *
            input.text-input.form-control name="contact_person" ng-model="user.fiocountry" required="" type="text" minlength="5" placeholder= t("post_product.full_name")
            p.help-block ng-show=("usubmitForm.fio.$invalid && !usubmitForm.fio.$pristine")  You fio is street.
          // email
          .form-group ng-class=("{ 'has-error' : usubmitForm.email.$invalid && !usubmitForm.email.$pristine }")
            .lable-one
              .label E-mail
              .span  *
            input.text-input.form-control name="email" ng-model="user.email" required="" type="email" placeholder="gmail@gmail.com" /
            p.help-block ng-show=("usubmitForm.email.$invalid && !usubmitForm.email.$pristine")  Enter a valid email.
          // phonefio
          .form-group ng-class=("{ 'has-error' : usubmitForm.phonefio.$invalid && !usubmitForm.phonefio.$pristine }")
            .lable-one
              .label
                = t("post_product.phone")
            input.text-input.form-control required="" name="contact_person_phone" ng-model="user.phonefio" type="text"  placeholder="+91-636-78658" ng-pattern="phoneNumbr"
            p.help-block.phone ng-show="usubmitForm.phonefio.$error.pattern"  Please match pattern [+91 036 78658 or 91 036 78658]
          //fax
          .form-group ng-class=("{ 'has-error' : usubmitForm.fax.$invalid && !usubmitForm.fax.$pristine }")
            .lable-one
              .label
                = t("post_product.fax")
            input.text-input.form-control required="" name="fax" ng-model="user.fax" type="text"  placeholder="+910367865844444" ng-pattern="faxNumbr"
            p.help-block.phone ng-show="usubmitForm.fax.$error.pattern"  Please match pattern [+910367865844444 or 910367865844444]
          .button-te
            button#submitotp.open-popup data-id="popup_default" ng-disabled="usubmitForm.$invalid" type="submit"
              = t("post_product.send")





